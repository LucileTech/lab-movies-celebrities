<ul>

  {{#each myMovie}}

    <li>
      <p>{{this.title}}</p>
      <p>{{this.genre}}</p>
      <p>{{this.plot}}</p>
      <ul>
        {{#each this.cast}}
          <li>

            <p>{{this.name}}</p>
            <p>{{this.occupation}}</p>
            <p>{{this.catchPhrase}}</p>

          </li>

        {{/each}}
      </ul>
    </li>

  {{/each}}

</ul>